{
  "extends": ["airbnb", "react-app", "prettier"],
  "rules": {
    "react/jsx-filename-extension": [
      1,
      { "extensions": [".js", ".jsx"] }
    ],
    "prettier/prettier": "error",
    "endOfLine": "crlf",
    "no-unused-vars": "warn",
    "no-console": "off",
    "func-names": "off",
    "no-process-exit": "off",
    "object-shorthand": "off",
    "class-methods-use-this": "off"
  },
  "plugins": ["prettier"]
}
